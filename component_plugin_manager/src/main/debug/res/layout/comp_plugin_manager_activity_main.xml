<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="debug.activity.CompPluginManagerActivity">


    <TextView
        android:id="@+id/comp_plugin_manager_tv_install"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="check and install" />

    <Space
        android:layout_width="wrap_content"
        android:layout_height="@dimen/material_16dp" />
    <TextView
        android:id="@+id/comp_plugin_manager_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="comp_plugin_manager_info" />

    <TextView
            android:id="@+id/comp_plugin_manager_info_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="" />


</LinearLayout>
